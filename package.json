{
	"name": "pcli",
	"type": "module",
	"scripts": {
		"build": "tsc -p tsconfig.json",
		"start": "ts-node-esm src/index.ts",
		"dev": "tsc-watch --noClear -p tsconfig.json",
		"prepack:esbuild": "npx esbuild dist/src/index.js  --bundle --outfile=dist/index.build.js --format=esm --platform=node --banner:js=\"import {createRequire} from 'module'; const require = createRequire(import.meta.url); import { dirname } from 'path'; import { fileURLToPath } from 'url'; const __dirname = dirname(fileURLToPath(import.meta.url));\"",
		"prepack:babel": "npx babel dist/src -d dist/babel",
		"prepack:webpack": "npx webpack --config webpack.config.cjs",
		"prepack:pkg": "npx pkg --options '--no-warnings' ./dist/webpack/main.js --out-path ./bin -C Brotli"
	},
	"dependencies": {
		"axios": "^0.27.2",
		"command-line-args": "^5.2.1",
		"commander": "^9.4.0",
		"dotenv": "^16.0.1",
		"enquirer": "^2.3.6",
		"esm": "^3.2.25",
		"file-type": "^17.1.6",
		"open-editor": "^4.0.0",
		"postman-collection": "^4.1.5",
		"prompt-sync": "^4.2.0",
		"strip-ansi": "^7.0.1",
		"tmp": "^0.2.1",
		"winston": "^3.8.1"
	},
	"devDependencies": {
		"@babel/cli": "^7.18.10",
		"@babel/core": "^7.18.10",
		"@babel/plugin-syntax-import-meta": "^7.10.4",
		"@babel/plugin-transform-modules-commonjs": "^7.18.6",
		"@babel/preset-env": "^7.18.10",
		"@babel/register": "^7.18.9",
		"@types/babel__register": "^7.17.0",
		"@types/command-line-args": "^5.2.0",
		"@types/command-line-usage": "^5.0.2",
		"@types/esm": "^3.2.0",
		"@types/lodash": "^4.14.182",
		"@types/postman-collection": "^3.5.7",
		"@types/prompt-sync": "^4.1.1",
		"@types/tmp": "^0.2.3",
		"command-line-usage": "^6.1.3",
		"release-it": "^15.3.0",
		"swc": "^1.0.11",
		"tsc-watch": "^5.0.3",
		"typescript": "^4.7.4",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0"
	}
}
